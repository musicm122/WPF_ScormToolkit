<UserControl
    x:Class="ScormToolkit_Wpf.Pages.SettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mui="http://firstfloorsoftware.com/ModernUI"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Pages/Settings/Scorm.xaml">
            <mui:ModernTab.Links>
                <!--<mui:Link DisplayName="appearance" Source="/Pages/Settings/Appearance.xaml" />-->
                <mui:Link DisplayName="scorm settings" Source="/Pages/Settings/Scorm.xaml" />
                <mui:Link DisplayName="license" Source="/Pages/Settings/License.xaml" />
                <mui:Link DisplayName="about" Source="/Pages/Settings/About.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>